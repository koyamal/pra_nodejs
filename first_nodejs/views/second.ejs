<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="/css/style.css">
    <title>Pra_Node.js(View Users)</title>
    <script src="/send_url.js"></script>
  </head>
  <body>
    <%- include('header.ejs'); %>
    <h1>Welcome to View Users page!</h1>
    <% var test = "This is " + " page " + itemId %>
    <%= test %>
    <p>itemId = <%= itemId %></p>
    <h3>Registered Users</h3>
    <table border="2">
      <tr>
        <th>Id</th><th>Name</th>
      </tr>
      <% usersInfo.forEach((userInfo =>{ %>
        <tr>
          <th>
            <%= userInfo.id %>
          </th>
          <th>
            <%= userInfo.name %>
          </th>
        </tr>
      <% })); %>      
    </table>
    <div class="btm">
      <a class="changepage underline" href="/">Top page!</a>
    </div>
    <%- include('footer.ejs'); %>
  </body>
</html>